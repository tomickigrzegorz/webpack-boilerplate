    "copy-webpack-plugin": "^6.2.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
